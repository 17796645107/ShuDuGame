<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mbp.sudoku">



    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.CheckPointActivity"/>

        <activity android:name=".activity.GameActivity"/>
        <activity android:name=".activity.AdminGenerateActivity" />
        <activity android:name=".activity.AdminOperateActivity" />
        <activity android:name=".activity.AdminSelectActivity" />
        <activity android:name=".activity.GameSuccessActivity" />
        <activity android:name=".activity.GameFailureActivity" />
        <activity android:name=".activity.SuspendActivity" />
    </application>

</manifest>